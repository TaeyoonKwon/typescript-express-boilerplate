{
  "name": "typescript-express-boilerplate",
  "version": "1.0.2",
  "description": "Production ready typescript express boilerplate",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc && tsc-alias",
    "lint": "eslint --ignore-path .gitignore --ext .ts src/",
    "lint:fix": "npm run lint --fix",
    "test": "jest",
    "dev": "tsc-watch --onSuccess  \"ts-node -r tsconfig-paths/register src/server.ts\"",
    "start": "node dist/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TaeyoonKwon/typescript-express-boilerplate.git"
  },
  "keywords": [
    "typescript",
    "express",
    "openapi",
    "mongoose",
    "boilerplate"
  ],
  "author": "Taeyoon Kwon <tabriell@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TaeyoonKwon/typescript-express-boilerplate/issues"
  },
  "homepage": "https://github.com/TaeyoonKwon/typescript-express-boilerplate#readme",
  "dependencies": {
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "envalid": "^7.3.1",
    "express": "^4.18.1",
    "express-openapi": "^11.0.1",
    "express-rate-limit": "^6.4.0",
    "helmet": "^5.1.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.3.4",
    "mongoose-paginate-v2": "^1.6.3",
    "morgan": "^1.10.0",
    "should": "^13.2.3",
    "swagger-jsdoc": "^6.2.1",
    "swagger-ui-express": "^4.4.0",
    "winston": "^3.7.2"
  },
  "devDependencies": {
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.5.1",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.35",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.25.0",
    "@typescript-eslint/parser": "^5.25.0",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^28.1.0",
    "prettier": "^2.6.2",
    "ts-jest": "^28.0.2",
    "ts-node": "^10.8.0",
    "tsc-alias": "^1.6.7",
    "tsc-watch": "^5.0.3",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.6.4"
  }
}
